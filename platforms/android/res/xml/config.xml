<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bubernak.flyfrenzy" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <name>FlyFrenzy</name>
    <description>
        So many flys. So little time.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="www/img/fly.png" />
    <icon density="ldpi" platform="android" src="www/img/fly.png" />
    <icon density="mdpi" platform="android" src="www/img/fly.png" />
    <icon density="hdpi" platform="android" src="www/img/fly.png" />
    <icon density="xhdpi" platform="android" src="www/img/fly.png" />
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
    </feature>
</widget>
